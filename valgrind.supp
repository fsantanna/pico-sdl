# Suppress false positives from external libraries

# glibc dynamic loader (invalid reads)
{
   glibc-ld-strncmp
   Memcheck:Addr8
   fun:strncmp
   fun:is_dst
   ...
}

# Library initialization (before user code runs)
{
   glibc-call-init
   Memcheck:Leak
   match-leak-kinds: all
   ...
   fun:call_init*
   ...
}

# SDL_Init leaks (pico.c:237)
{
   sdl-init
   Memcheck:Leak
   match-leak-kinds: all
   ...
   src:pico.c:237
}
